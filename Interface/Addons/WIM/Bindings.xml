<Bindings>
	<Binding name="WIMENABLE" header="WIMMOD">
			WIM_Bindings_EnableWIM();
	</Binding>
	<Binding name="WIMHISTORY" header="WIMMOD">
			WIM_HistoryFrame:Show();
	</Binding>
	<Binding name="WIMSHOWNEW" header="WIMMOD">
			WIM_ShowNewMessages();
	</Binding>
	<Binding name="WIMTOGGLE" header="WIMMOD">
			WIM_ToggleWindow_Toggle();
	</Binding>
	<Binding name="WIMSHOWALL" header="WIMMOD">
			WIM_ShowAll();
	</Binding>
	<Binding name="WIMHIDEALL" header="WIMMOD">
			WIM_HideAll();
	</Binding>
</Bindings>
